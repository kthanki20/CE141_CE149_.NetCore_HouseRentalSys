@model IEnumerable<RentalSys.Models.HouseBookingModel>

@{
    ViewData["Title"] = "Booked Houses";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}



<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">

            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Booked House Details</h4>
                        <p class="card-description">

                        </p>
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>Customer Name</th>
                                        <th>Customer Address</th>
                                        <th>Email</th>
                                        <th>Phone Number</th>
                                      
                                        <th>CNIC No</th>
                                       

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.bCusName)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.bCusAddress)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.bCusEmail)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.bCusPhoneNum)
                                            </td>
                                            
                                            <td>
                                                @Html.DisplayFor(modelItem => item.bCusCnic)
                                            </td>
                                            @* <td>
                                                <form asp-action="Details" asp-controller="FlightBooking">
                                                    <input type="hidden" value="@item.ResId" name="id" />
                                                    <input type="submit" value="View" class="btn btn-primary" />
                                                   
                                                </form>


                                            </td> *@

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


