@model RentalSys.Models.HouseDetailModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}


<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Booked House Information</h4>
                        <p class="card-description">

                        </p>
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>@Html.DisplayNameFor(model => model.HouseName)</th>
                                        <th>@Html.DisplayNameFor(model => model.HouseType)</th>
                                        <th>@Html.DisplayNameFor(model => model.HouseRentAmount)</th>
                                        <th>@Html.DisplayNameFor(model => model.HouseAddress)</th>
                                        

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(model => model.HouseName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.HouseType)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.HouseRentAmount)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.HouseAddress)
                                        </td>
                                      

                                    </tr>

                                    <tr>
                                    </tr>

                                </tbody>


                            </table>
                            <div style="margin:20px">
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

